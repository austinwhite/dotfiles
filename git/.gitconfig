[user]
    name = Austin White
    email = austinlldb@gmail.com
[submodule]
    recurse = true
[credential]
    helper = store
[alias]
    cl = clone --recurse
    co = checkout
    fo = fetch origin
    pl = pull
    ci = commit
    br = branch
    ps = push
    st = status -sb
    lg = log --pretty=oneline -n 20 --graph --abbrev-commit
    aliases = config --get-regexp alias
    amend = commit --amend --reuse-message=HEAD
    unstage = reset HEAD --
    reset = reset --soft HEAD~1
[core]
    editor = nvim
    excludesfile = /home/austin/.config/git/.global-gitignore
[color]
    ui = auto
[color "status"]
    added = yellow
    changed = green
    untracked = cyan
[color "branch"]
    current = yellow reverse
    local = yellow
    remote = green
[color "diff"]
    meta = yellow bold
    frag = magenta bold
    old = red
    new = green
[init]
    defaultbranch = main
